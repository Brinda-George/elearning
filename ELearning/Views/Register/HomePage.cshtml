@model ELearning.Models.MaterialModel

@{
    ViewBag.Title = "HomePage";
}



<h2>Welcome @TempData["user"]</h2>
<br /><br />
@using (Html.BeginForm("Search", "Register"))
{
    @*@Html.AntiForgeryToken()*@
    <div class="form-horizontal">
        <div class="form-group">
            @*<label class="control-label col-md-2" style="font-size:16px;">Select Technology</label>*@
            @Html.LabelFor(model => model.technology, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("Technology", "-- select --")
                @Html.ValidationMessageFor(model => model.technology, "", new { @class = "text-danger" })
            </div>
        </div>
       
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Search" class="btn btn-default" />
            </div>
        </div>
    </div>
}
